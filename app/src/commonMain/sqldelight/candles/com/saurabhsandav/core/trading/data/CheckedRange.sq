import kotlin.time.Instant;

CREATE TABLE IF NOT EXISTS CheckedRange (
tableName TEXT NOT NULL UNIQUE,
fromEpochSeconds INTEGER AS Instant NOT NULL,
toEpochSeconds INTEGER AS Instant NOT NULL
);

insert:
INSERT OR REPLACE INTO CheckedRange(tableName, fromEpochSeconds, toEpochSeconds)
VALUES (?, ?, ?);

get:
SELECT * FROM CheckedRange
WHERE tableName = ?;
