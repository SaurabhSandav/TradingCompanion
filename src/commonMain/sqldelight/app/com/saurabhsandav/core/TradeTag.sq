
CREATE TABLE IF NOT EXISTS TradeTag (
id INTEGER AS Long NOT NULL PRIMARY KEY,
name TEXT NOT NULL UNIQUE,
UNIQUE (name COLLATE NOCASE)
);

insert:
INSERT OR IGNORE INTO TradeTag
VALUES (?, ?);

get:
SELECT * FROM TradeTag
WHERE id = ?;

getAll:
SELECT * FROM TradeTag;

delete:
DELETE FROM TradeTag
WHERE id = ?;
