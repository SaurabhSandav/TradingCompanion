
CREATE TABLE IF NOT EXISTS ClosedTradeDetail (
closedTradeId INTEGER AS Int NOT NULL UNIQUE,
maxFavorableExcursion TEXT NOT NULL,
maxAdverseExcursion TEXT NOT NULL,
persisted TEXT NOT NULL,
persistenceResult TEXT,
FOREIGN KEY (closedTradeId) REFERENCES ClosedTrade(id)
);

insert:
INSERT
INTO ClosedTradeDetail(closedTradeId, maxFavorableExcursion, maxAdverseExcursion, persisted, persistenceResult)
VALUES (?, ?, ?, ?, ?);
