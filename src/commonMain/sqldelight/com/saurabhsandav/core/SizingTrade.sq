
CREATE TABLE IF NOT EXISTS SizingTrade (
id INTEGER AS Long NOT NULL PRIMARY KEY,
ticker TEXT NOT NULL,
entry TEXT NOT NULL,
stop TEXT NOT NULL
);

insert:
INSERT OR REPLACE INTO SizingTrade
VALUES (?, ?, ?, ?);

getAll:
SELECT * FROM SizingTrade;

updateEntry:
UPDATE SizingTrade
SET entry = ?
WHERE id = ?;

updateStop:
UPDATE SizingTrade
SET stop = ?
WHERE id = ?;

delete:
DELETE FROM SizingTrade
WHERE id = ?;
